// https://typedoc.org/options/
{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-md", "typedoc-plugin-markdown"],
  "githubPages": false,
  "treatWarningsAsErrors": true,
  "cleanOutputDir": true,
  "disableSources": true,

  "excludeExternals": true,
  "excludeInternal": true,
  "excludePrivate": true,
  "validation": {
    "notDocumented": true
  },
  "requiredToBeDocumented": [
    "Enum"
    // "EnumMember",
    // "Variable"
    // "Function",
    // "Class",
    // "Interface",
    // "Property",
    // "Method",
    // "Accessor",
    // "TypeAlias"
  ],
  "visibilityFilters": {
    "protected": false,
    "private": false,
    "inherited": false,
    "external": false,
    "@alpha": false,
    "@beta": false
  },

  "readme": "none"
}
