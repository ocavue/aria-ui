{
  "name": "@aria-ui/menu",
  "type": "module",
  "version": "0.0.14",
  "private": false,
  "sideEffects": false,
  "main": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./elements": "./src/elements.ts"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup"
  },
  "dependencies": {
    "@aria-ui/collection": "workspace:^",
    "@aria-ui/core": "workspace:^",
    "@aria-ui/overlay": "workspace:^",
    "@aria-ui/popover": "workspace:^",
    "@aria-ui/presence": "workspace:^",
    "nanoid": "^5.0.8"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./elements": {
        "types": "./dist/elements.d.ts",
        "import": "./dist/elements.js"
      }
    }
  }
}
